{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"target": "es5",
		"noImplicitAny": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"baseUrl": ".",
		"jsx": "react",
		"allowJs": true,
		"rootDir": "src",
		"types": [
			"react",
			"react-dom"
		]
	},
	"include": [
		"src/**/*",
	],
	"exclude": [
		"node_modules"
	]
}